{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "functions": {
    "main.py": {
      "runtime": "python3.11"
    }
  },
  "installCommand": "pip install -r requirements.txt",
  "buildCommand": "",
  "outputDirectory": "",
  "framework": null
}
